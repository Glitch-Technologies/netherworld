{
    "container": {
        "display": "grid",
        "grid-template-columns": "1fr 1fr 1fr 1fr 1fr",
        "grid-template-rows": "1fr 1fr 1fr",
        "height": "100vh"
    },
    "left": {
        "grid-column": "1",
        "grid-row": "1 / span 3",
        "background-color": "#b2b2b2",
        "position": "relative"
    },
    "right": {
        "grid-column": "5",
        "grid-row": "1 / span 3",
        "background-color": "#b2b2b2",
        "position": "relative"
    },
    "bottom": {
        "grid-column": "2 / span 3",
        "grid-row": "3",
        "background-color": "#f2f2f2",
        "position": "relative"
    },
    "middle": {
        "grid-column": "2 / span 3",
        "grid-row": "1 / span 2",
        "background-color": "#a0a0a0",
        "position": "relative"
    }
}